<% if user_signed_in? %>
  <% if user.id != current_user.id %>
    <% if @haveRoom == true %>
      <%= link_to "DMを始める", room_path(roomId) %>
    <% else%>
      <%= form_with(model:@room, local: true) do |f| %>
        <%= fields_for @entry do |e| %>
          <%= e.hidden_field :user_id, value: user.id %>
          <%= f.submit "DMを始める" %>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
<% end %>